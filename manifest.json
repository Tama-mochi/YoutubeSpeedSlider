{
    "manifest_version": 3,
    "name": "YouTube Speed Slider",
    "version": "1.1",
    "description": "Adds a playback speed slider on the YouTube player.",
    "permissions": [],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }